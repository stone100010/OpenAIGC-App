# Progress SQL数据库配置
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=iflow_app
DATABASE_USER=postgres
DATABASE_PASSWORD=your_password

# Redis缓存配置（可选）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# 应用配置
APP_ENV=development
APP_SECRET=your_app_secret_key
APP_URL=http://localhost:3000

# ModelScope API配置 - 图像生成
# 请在.env文件中设置您的真实API密钥，不要使用示例密钥
# MODELSCOPE_API_KEY=your_actual_api_key_here

# Pollinations API配置 - 音频生成
# NEXT_PUBLIC_POLLINATIONS_TOKEN=your_pollinations_token_here

# 文件存储配置
UPLOAD_MAX_SIZE=10485760
STATIC_FILE_URL=http://localhost:3000/uploads

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log